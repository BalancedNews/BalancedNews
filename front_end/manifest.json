{
  "background": {
    "scripts": [
      "src/js/background.js",
      "src/js/popup.js"
    ]
  },
  "page_action": {
    "default_popup": "src/html/popup.html",
    "default_title" : "Click to read balanced news!",
    "default_icon": {
      "128": "images/balanced_news_icon.png"
    }
  },
  "description": "Read news all across the political spectrum",
  "homepage_url": "https://www.linkedin.com/in/carol-yueming-chen/",
  "icons": {
    "128": "images/balanced_news_icon.png"
  },
  "manifest_version": 2,
  "name": "Balanced News",
  "short_name": "Balanced News",
  "options_ui": {
    "chrome_style": false,
    "page": "src/html/options.html"
  },
  "permissions": [
    "notifications",
    "tabs",
    "webRequest",
    "declarativeContent"
  ],
  "version": "1.7.4"
}
